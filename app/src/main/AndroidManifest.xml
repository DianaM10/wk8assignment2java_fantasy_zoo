<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.fantasyzoo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name=".Zootopia"
            android:label="@string/app_name">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>

            </activity>

        <activity
            android:name=".ZooResultActivity"
            android:label="@string/zoo_result_activity_title"
            android:parentActivityName=".Zootopia">
        </activity>

        <activity
            android:name=".EnclosureResultActivity"
            android:label="@string/result_activity_title"
            android:parentActivityName=".ZooResultActivity">
        </activity>

        <activity
            android:name=".BuyCreatureActivity"
            android:label="@string/result_activity_title"
            android:parentActivityName=".BuyCreatureActivity">
        </activity>

    </application>

</manifest>
